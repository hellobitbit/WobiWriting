<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TargetToolBar">
        <attr name="textSelectedColor" format="color"/>
        <attr name="textNoSelectedColor" format="color"/>
        <attr name="textSize" format="dimension"/>
        <attr name="SelectedSrc" format="reference"/>
        <attr name="NoSelectedSrc" format="reference"/>
        <attr name="textValue" format="string"/>
        <attr name="isSelected" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomSettingBar">
        <attr name="rightIconSrc" format="reference"/>
        <attr name="rightIconMarginRight" format="dimension"/>
        <attr name="rightIconSize" format="dimension"/>
        <attr name="leftIconSrc" format="reference"/>
        <attr name="leftIconMarginLeft" format="dimension"/>
        <attr name="leftIconSize" format="dimension"/>
        <attr name="leftIconVisible" format="boolean"/>
        <attr name="leftTextValue" format="string"/>
        <attr name="leftTextColor" format="color"/>
        <attr name="leftTextMarginLeft" format="dimension"/>
        <attr name="leftTextSize" format="dimension"/>
        <attr name="rightTextValue" format="string"/>
        <attr name="rightTextColor" format="color"/>
        <attr name="rightTextMarginRight" format="dimension"/>
        <attr name="rightTextSize" format="dimension"/>
    </declare-styleable>
</resources>
